        <div id="loadingContent">
            <div id="loadSpinnerContainer">
                <img id="loadCenterImage" src="images/LoadingText.png">
                <img id="loadSpinnerImage" class="rotating" src="images/LoadingSeal.png">
            </div>
        </div>
    </div>
